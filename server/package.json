{
	"name": "server",
	"private": true,
	"version": "1.0.5",
	"description": "Recipe page server",
	"main": "index.js",
	"scripts": {
		"start:dev": "nodemon",
		"start:prod": "cross-env NODE_ENV=production ts-node src/main.ts",
		"start:build": "npx tsc",
		"start:lint": "eslint src/**/*.ts",
		"start:format": "eslint src/**/*.ts --fix",
		"start:pretty": "prettier --write \"src/**/*.ts\""
	},
	"keywords": [
		"Express",
		"MVC",
		"MongoDb",
		"Mongoose"
	],
	"author": "Edanriell",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.20.2",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.1",
		"express-rate-limit": "^7.2.0",
		"helmet": "^7.1.0",
		"mongodb": "^6.5.0",
		"mongoose": "^8.2.3",
		"rate-limiter": "^0.2.0",
		"toobusy-js": "^0.5.1",
		"winston": "^3.12.0",
		"winston-daily-rotate-file": "^5.0.0"
	},
	"devDependencies": {
		"@tsconfig/node20": "^20.1.2",
		"@types/body-parser": "^1.19.5",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/express-rate-limit": "^6.0.0",
		"@types/node": "^20.11.30",
		"@types/toobusy-js": "^0.5.4",
		"@typescript-eslint/eslint-plugin": "^7.3.1",
		"@typescript-eslint/parser": "^7.3.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.0",
		"eslint-plugin-security": "^2.1.1",
		"nodemon": "^3.1.0",
		"prettier": "^3.2.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.3"
	}
